<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iitd.sahyog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivitySignIn"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityFeed"
            android:label="@string/title_activity_main_feed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivityRegistration"
            android:label="@string/title_activity_main_registration"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>